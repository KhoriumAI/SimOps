FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant/fluid";
    object      thermophysicalProperties;
}

// ========================================================================== //
// Golden CHT Case - Fluid (Air) Thermophysical Properties                    //
// ========================================================================== //

thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       sutherland;
    thermo          hConst;
    equationOfState perfectGas;
    specie          specie;
    energy          sensibleEnthalpy;
}

mixture
{
    specie
    {
        molWeight   28.9;  // Air molecular weight [kg/kmol]
    }
    thermodynamics
    {
        // VARIABLE: Cp - Specific heat of air
        Cp          1005;   // [J/kg/K]
        Hf          0;
    }
    transport
    {
        as  1.4578e-6;
        Ts  110.4;
    }
}

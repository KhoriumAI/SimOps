FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "600/solid_chip";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 100000;

boundaryField
{
    chip_top
    {
        type            calculated;
        value           uniform 100000;
    }
    defaultFaces
    {
        type            calculated;
        value           uniform 100000;
    }
    solid_chip_to_solid_heatsink
    {
        type            calculated;
        value           uniform 100000;
    }
}


